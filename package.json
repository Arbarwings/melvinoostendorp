{
  "name": "melvinoostendorp",
  "private": true,
  "description": "The portfolio website for melvinoostendorp.nl",
  "version": "0.1.9",
  "author": "Melvin Oostendorp <info@arbarwings.nl>",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint \"./**/*.jsx\" \"./**/*.js\"",
    "stylelint": "stylelint \"./**/*.scss\"",
    "lint": "npm run eslint && npm run stylelint",
    "format:js": "npm run eslint -- --fix",
    "format:scss": "npm run stylelint -- --fix",
    "format:md": "prettier --write README.md",
    "format": "npm run format:js && npm run format:scss && npm run format:md",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public -b website -u \"GitHub <noreply@github.com>\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "get-version": "echo $npm_package_version",
    "update-version": "release-it --ci --no-git --no-npm.publish",
    "create-changelog": "auto-changelog --template changelog-template.hbs --starting-version v$npm_package_version"
  },
  "dependencies": {
    "@mdi/js": "6.5.95",
    "@mdi/react": "1.5.0",
    "bulma": "0.9.3",
    "classnames": "2.3.1",
    "gatsby": "4.9.3",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-google-analytics": "4.9.0",
    "gatsby-plugin-manifest": "4.9.1",
    "gatsby-plugin-nprogress": "4.9.0",
    "gatsby-plugin-offline": "5.9.1",
    "gatsby-plugin-postcss": "5.9.0",
    "gatsby-plugin-react-helmet": "5.9.0",
    "gatsby-plugin-robots-txt": "1.7.0",
    "gatsby-plugin-sass": "5.9.0",
    "gatsby-plugin-sharp": "4.9.1",
    "gatsby-plugin-sitemap": "5.9.0",
    "gatsby-transformer-sharp": "4.9.0",
    "node-sass": "7.0.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-scroll": "1.8.6",
    "react-tsparticles": "1.42.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/eslint-parser": "7.17.0",
    "@babel/eslint-plugin": "7.16.5",
    "@babel/preset-react": "7.16.7",
    "auto-changelog": "2.4.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.3",
    "gh-pages": "3.2.3",
    "prettier": "2.5.1",
    "release-it": "14.12.5",
    "stylelint": "14.5.3",
    "stylelint-config-recommended-scss": "5.0.2",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arbarwings/melvinoostendorp"
  },
  "bugs": {
    "url": "https://github.com/arbarwings/melvinoostendorp/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
