{
  "name": "melvinoostendorp",
  "private": true,
  "description": "The portfolio website for melvinoostendorp.nl",
  "version": "0.1.6",
  "author": "Melvin Oostendorp <info@arbarwings.nl>",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint \"./**/*.jsx\" \"./**/*.js\"",
    "stylelint": "stylelint \"./**/*.scss\"",
    "lint": "npm run eslint && npm run stylelint",
    "format:js": "npm run eslint -- --fix",
    "format:scss": "npm run stylelint -- --fix",
    "format:md": "prettier --write README.md",
    "format": "npm run format:js && npm run format:scss && npm run format:md",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public -b website -u \"GitHub <noreply@github.com>\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "get-version": "echo $npm_package_version",
    "update-version": "release-it --ci --no-git --no-npm.publish",
    "create-changelog": "auto-changelog --template changelog-template.hbs --starting-version v$npm_package_version"
  },
  "dependencies": {
    "@mdi/js": "5.9.55",
    "@mdi/react": "1.5.0",
    "bulma": "0.9.2",
    "classnames": "2.3.1",
    "gatsby": "3.4.1",
    "gatsby-image": "3.4.0",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-google-analytics": "3.4.0",
    "gatsby-plugin-manifest": "3.4.0",
    "gatsby-plugin-nprogress": "3.4.0",
    "gatsby-plugin-offline": "4.4.0",
    "gatsby-plugin-postcss": "4.4.0",
    "gatsby-plugin-react-helmet": "4.4.0",
    "gatsby-plugin-robots-txt": "1.5.6",
    "gatsby-plugin-sass": "4.4.0",
    "gatsby-plugin-sharp": "3.4.1",
    "gatsby-plugin-sitemap": "4.0.0",
    "gatsby-transformer-sharp": "3.4.0",
    "node-sass": "5.0.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-scroll": "1.8.2",
    "react-tsparticles": "1.26.4"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/eslint-parser": "7.13.14",
    "@babel/eslint-plugin": "7.13.16",
    "@babel/preset-react": "7.13.13",
    "auto-changelog": "2.2.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "gh-pages": "3.1.0",
    "prettier": "2.2.1",
    "release-it": "14.6.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-declaration-use-variable": "1.7.2",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "3.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arbarwings/melvinoostendorp"
  },
  "bugs": {
    "url": "https://github.com/arbarwings/melvinoostendorp/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
