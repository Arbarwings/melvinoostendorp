{
  "name": "melvinoostendorp",
  "private": true,
  "description": "The portfolio website for melvinoostendorp.nl",
  "version": "0.1.0",
  "author": "Melvin Oostendorp <info@arbarwings.nl>",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint \"./**/*.jsx\" \"./**/*.js\"",
    "stylelint": "stylelint \"./**/*.scss\"",
    "lint": "npm run eslint && npm run stylelint",
    "format:js": "npm run eslint -- --fix",
    "format:scss": "npm run stylelint -- --fix",
    "format:md": "prettier --write README.md",
    "format": "npm run format:js && npm run format:scss && npm run format:md",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public -b website -u \"GitHub <noreply@github.com>\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "get-version": "echo $npm_package_version",
    "update-version": "release-it --ci --no-git --no-npm.publish",
    "create-changelog": "auto-changelog --template changelog-template.hbs --starting-version v$npm_package_version"
  },
  "dependencies": {
    "@mdi/js": "5.4.55",
    "@mdi/react": "1.4.0",
    "bulma": "0.9.0",
    "classnames": "2.2.6",
    "gatsby": "2.24.23",
    "gatsby-image": "2.4.14",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-google-analytics": "2.3.13",
    "gatsby-plugin-manifest": "2.4.21",
    "gatsby-plugin-nprogress": "2.3.10",
    "gatsby-plugin-offline": "3.2.21",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-sass": "2.3.12",
    "gatsby-plugin-sharp": "2.6.24",
    "gatsby-plugin-sitemap": "2.4.11",
    "gatsby-transformer-sharp": "2.5.12",
    "node-sass": "4.14.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-scroll": "1.8.0",
    "react-tsparticles": "1.17.5"
  },
  "devDependencies": {
    "auto-changelog": "2.2.0",
    "eslint": "7.6.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "gh-pages": "3.1.0",
    "prettier": "2.0.5",
    "release-it": "13.6.6",
    "stylelint": "13.6.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-declaration-use-variable": "1.7.2",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arbarwings/melvinoostendorp"
  },
  "bugs": {
    "url": "https://github.com/arbarwings/melvinoostendorp/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
